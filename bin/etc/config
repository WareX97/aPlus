# -- /etc/config
# -- aPlus config




# -- System
sys.locale=it-IT
sys.startup=/usr/bin/sh
sys.shell=/usr/bin/sh


# -- Keyboard
kb.enabled=1
kb.keymap=$sys.locale


# -- Screen
screen.width=1280
screen.height=768
screen.bpp=32


# -- Energy saving
idle.priority.disabled=0
idle.priority.low=1
idle.priority.medium=2
idle.priority.high=3
idle.priority=$idle.priority.disabled


# -- I/O Scheduler
iosched.priority=0
iosched.blksize=4194304


# -- UI
ui.dpi=72
ui.font=Arial
ui.wallpaper=/usr/share/images/wp.png

