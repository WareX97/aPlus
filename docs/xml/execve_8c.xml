        <type>int</type>
        <definition>int sys_execve</definition>
        <argsstring>(char *filename, char **argv, char **environ)</argsstring>
        <name>sys_execve</name>
        <param>
          <type>char *</type>
          <declname>filename</declname>
        </param>
        <param>
          <type>char **</type>
          <declname>argv</declname>
        </param>
